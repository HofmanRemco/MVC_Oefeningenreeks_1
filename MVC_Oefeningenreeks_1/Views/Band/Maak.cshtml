
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Maak</title>
</head>
<body>
    <table class="table">
        <thead>
            <tr>
                <th>Band</th>
                <th>Jaar</th>
                <th>Link</th>
            </tr>
        </thead>
        <tbody>
            @{
                <tr>
                    <td>@ViewBag.band.Naam</td>
                    <td>@ViewBag.band.Jaar</td>
                    <td><a asp-controller="Band" asp-action="Lijst" asp-fragment="@ViewBag.band.Naam">Link</a></td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
